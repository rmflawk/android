        <param name="android-package" value="com.raccoondev85.plugin.kakao.KakaoCordovaSDK" />
    </feature>
    <access origin="https://*.kakao.com" />
    <access origin="https://*.kakao.co.kr" />
    <access origin="https://*.kakaocdn.net" />
    <feature name="GooglePlus">
        <param name="android-package" value="nl.xservices.plugins.GooglePlus" />
        <param name="onload" value="true" />
    </feature>
    <access origin="https://accounts.google.com/*" />
    <feature name="UnlocksDetector">
        <param name="android-package" value="cordova.plugins.UnlocksDetector" />
        <param name="onload" value="true" />
